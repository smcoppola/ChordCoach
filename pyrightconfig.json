{
    "include": [
        "src"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "venv"
    ],
    "venvPath": ".",
    "venv": "venv",
    "reportMissingImports": true,
    "typeCheckingMode": "basic",
    "pythonVersion": "3.14",
    "pythonPlatform": "Windows",
    "extraPaths": [
        "src",
        "build/src/hardware/Release"
    ]
}